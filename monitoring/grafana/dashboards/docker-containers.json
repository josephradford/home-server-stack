{
  "id": null,
  "title": "Docker Container Monitoring",
  "tags": [
    "docker",
    "cadvisor"
  ],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Container CPU Usage",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{name!=\"\"}[5m]) * 100",
          "legendFormat": "{{name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Container Memory Usage",
      "type": "graph",
      "targets": [
        {
          "expr": "container_memory_usage_bytes{name!=\"\"}",
          "legendFormat": "{{name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Container Network I/O",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{name!=\"\"}[5m])",
          "legendFormat": "Receive - {{name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "rate(container_network_transmit_bytes_total{name!=\"\"}[5m])",
          "legendFormat": "Transmit - {{name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "Container Status",
      "type": "stat",
      "targets": [
        {
          "expr": "count by (name) (container_last_seen{name!=\"\"})",
          "legendFormat": "{{name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s",
  "schemaVersion": 27,
  "version": 1
}